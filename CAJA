npts 40 40 40                        # num.grid points in xyz
gridfld proteinE_model1.maps.fld     # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD N OA           # receptor atom types
ligand_types A C HD N NA OA SA       # ligand atom types
receptor proteinE_model1.pdbqt       # macromolecule
gridcenter 5.93 -0.346 0.023         # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map proteinE_model1.A.map            # atom-specific affinity map
map proteinE_model1.C.map            # atom-specific affinity map
map proteinE_model1.HD.map           # atom-specific affinity map
map proteinE_model1.N.map            # atom-specific affinity map
map proteinE_model1.NA.map           # atom-specific affinity map
map proteinE_model1.OA.map           # atom-specific affinity map
map proteinE_model1.SA.map           # atom-specific affinity map
elecmap proteinE_model1.e.map        # electrostatic potential map
dsolvmap proteinE_model1.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
